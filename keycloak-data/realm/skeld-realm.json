{
  "realm": "skeld",
  "enabled": true,
  "registrationAllowed": true,
  "sslRequired": "none",
  "identityProviders": [
    {
      "alias": "dex",
      "displayName": "Dex OIDC",
      "providerId": "oidc",
      "enabled": true,
      "trustEmail": true,
      "storeToken": false,
      "addReadTokenRoleOnCreate": false,
      "authenticateByDefault": false,
      "linkOnly": false,
      "firstBrokerLoginFlowAlias": "first broker login",
      "config": {
        "clientId": "keycloak-client",
        "clientSecret": "changeme",
        "authorizationUrl": "http://localhost:5556/dex/auth",
        "tokenUrl": "http://dex:5556/dex/token",
        "userInfoUrl": "http://dex:5556/dex/userinfo",
        "jwksUrl": "http://dex:5556/dex/keys",
        "issuer": "http://dex:5556/dex",
        "defaultScope": "openid profile email",
        "syncMode": "IMPORT",
        "useJwksUrl": "true",
        "validateSignature": "true"
      }
    }
  ],
  "users": [
    {
      "id": "11111111-2222-3333-4444-555555555555",
      "username": "devuser",
      "enabled": true,
      "email": "devuser@example.com",
      "emailVerified": true,
      "firstName": "Dev",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": [
        "user"
      ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Default user role"
      },
      {
        "name": "admin",
        "description": "Administrator role"
      }
    ]
  },
  "clients": [
    {
      "clientId": "myclient",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "redirectUris": [
        "http://localhost:32678/*"
      ],
      "webOrigins": [
        "+"
      ],
      "defaultClientScopes": [
        "profile",
        "email"
      ],
      "optionalClientScopes": [
        "Products.Read",
        "Products.Write"
      ]
    },
    {
      "clientId": "api-server",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "bearerOnly": true,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "defaultClientScopes": [
        "profile",
        "email"
      ],
      "optionalClientScopes": [
        "Products.Read",
        "Products.Write"
      ]
    },
    {
      "clientId": "ui-client",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "changeme",
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "redirectUris": [
        "http://localhost:8080/login/oauth2/code/keycloak",
        "http://localhost:8080/logged-out"
      ],
      "webOrigins": [
        "http://localhost:8080"
      ],
      "defaultClientScopes": [
        "profile",
        "email"
      ],
      "optionalClientScopes": [
        "Products.Read",
        "Products.Write"
      ]
    }
  ],
  "clientScopes": [
    {
      "name": "profile",
      "description": "Default profile scope",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true"
      },
      "protocolMappers": [
        {
          "name": "preferred username",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "username",
            "claim.name": "preferred_username",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "given name",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "firstName",
            "claim.name": "given_name",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "family name",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "lastName",
            "claim.name": "family_name",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "full name",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-full-name-mapper",
          "config": {
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        }
      ]
    },
    {
      "name": "email",
      "description": "Default email scope",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true"
      },
      "protocolMappers": [
        {
          "name": "email",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "user.attribute": "email",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "claim.name": "email",
            "jsonType.label": "String"
          }
        },
        {
          "name": "email verified",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "user.attribute": "emailVerified",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "claim.name": "email_verified",
            "jsonType.label": "boolean"
          }
        }
      ]
    },
    {
      "name": "Products.Read",
      "description": "Allows reading product information",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true",
        "consent.screen.text": "Allows reading product information"
      }
    },
    {
      "name": "Products.Write",
      "description": "Allows writing product information",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true",
        "consent.screen.text": "Allows writing product information"
      }
    }
  ]
}